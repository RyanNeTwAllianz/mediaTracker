{
	"name": "Media hits",
	"description": "List all media hits",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
    	"default_popup": "index.html",
		"default_popup_width": 790
  	},
	"permissions": [
		"webRequest",
		"activeTab",
		"scripting",
		"tabs"
	],
	"host_permissions": [
		"http://*/*",
		"https://*/*"
	],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"icons": {
    	"48": "icon.png"
  	}
}
